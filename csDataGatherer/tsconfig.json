{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./Scripts/typings/angularjs/angular-cookies.d.ts",
        "./Scripts/typings/angularjs/angular-mocks.d.ts",
        "./Scripts/typings/angularjs/angular.d.ts",
        "./public/app/app.ts",
        "./server.ts"
    ]
}
